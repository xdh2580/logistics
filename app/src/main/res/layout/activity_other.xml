<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.logistics.OtherActivity">

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        <TextView
                android:text="其他功能"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/textView30" android:textAlignment="center"
                android:textSize="20sp"/>
        <Button
                android:text="导出数据库文件"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/button_other_export"/>
        <Button
                android:text="导入数据库文件"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/button_other_import"/>
        <Button
                android:text="查看日志记录"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/button_other_check"/>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>